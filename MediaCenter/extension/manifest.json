{
  "manifest_version": 3,
  "name": "AkOS Navbar",
  "version": "1.0",
  "description": "Injects AkOS navbar (skips akosaf.github.io)",
  "host_permissions": ["<all_urls>"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["navbar.css"],
      "js": ["navbar.js"],
      "run_at": "document_start"
    }
  ]
}